#include <iostream>
using namespace std;

int gcd(int a, int b) {
    while (b != 0) {
        int temp = a % b;
        a = b;
        b = temp;
    }
    return a;
}

int main() {
    int x, y, z;
    cout << "entre three nummbers: ";
    cin >> x >> y >> z;

    int result = gcd(gcd(x, y), z);

    cout << "node of three numbers = " << result << endl;
    return 0;
}
}